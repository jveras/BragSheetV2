# CREATE DATABASE SCRIPTS



DROP TABLE account;

CREATE table account(
    ID int NOT NULL UNIQUE AUTO_INCREMENT PRIMARY KEY,
    email VARCHAR(64) NOT NULL,
    password VARCHAR(256) NOT NULL,
    OSIS INT NOT NULL UNIQUE,
    verify INT NOT NULL,
    profileID INT NOT NULL
);

